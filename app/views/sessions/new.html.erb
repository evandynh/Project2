
<div>
<h1>Log In</h1>
  <%= form_for :login do |f| %>
  <div class="field form-group">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field form-group">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <%= f.submit 'Log In', class: 'btn btn-primary' %>

  <%= link_to 'Home', users_path, class: 'btn btn-primary' %>

  <% end %>
</div>
